{
  "name": "nuclui",
  "version": "0.0.5",
  "description": "A React UI framework, built with TypeScript, to make your developement easier so you can give more focus on your app and less on the design!",
  "main": "dist/index.js",
  "scripts": {
    "start:prod": "node docs/dist-server/server.js",
    "start:dev": "webpack-dev-server --config=\"webpack.config.ts\" --host 0.0.0.0 --env.NODE_ENV=development --port=3000",
    "clean": "rm -rf dist package docs/public/dist docs/dist-server *.tsbuildinfo coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings 0",
    "prettier:report": "prettier --check --config .prettierrc.js .",
    "test": "npm-run-all prettier:report lint && jest",
    "test:watch": "npm run lint && jest --watch",
    "test:publish": "npm run prepublishOnly && npm pack && tar xvzf *.tgz && rm -rf *.tgz && npm run postpublish",
    "test:heroku": "npm run heroku-postbuild && heroku local web",
    "compile": "npm-run-all clean test && ttsc --project src",
    "compile:scripts": "tsc --project scripts",
    "build:docs:dev": "npm-run-all clean test && webpack --env.NODE_ENV=development --config=\"webpack.config.ts\"",
    "build:docs:prod": "npm-run-all clean test && webpack -p --env.NODE_ENV=production --config=\"webpack.config.ts\"",
    "prepublishOnly": "npm-run-all compile && node scripts/compiled/prepublishOnly.js",
    "postpublish": "node scripts/compiled/postpublish.js",
    "heroku-postbuild": "npm run build:docs:prod && tsc --project docs/server",
    "changelog": "auto-changelog -p -u -l 20 -b 20 --hide-credit",
    "preversion": "npm test",
    "version": "npm run changelog && git add CHANGELOG.md",
    "createVersion": "node scripts/compiled/createVersion.js",
    "createPublish": "node scripts/compiled/createPublish.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxijonson/nuclui.git"
  },
  "keywords": [
    "react",
    "components",
    "library",
    "framework",
    "UI",
    "front-end",
    "typescript"
  ],
  "author": "Tristan Chin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/maxijonson/nuclui/issues"
  },
  "homepage": "https://github.com/maxijonson/nuclui#readme",
  "files": [
    "dist",
    "docs/public/assets/images/logo*.png"
  ],
  "peerDependencies": {
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "dependencies": {
    "express": "4.17.1",
    "express-sslify": "1.2.0",
    "lodash": "4.17.15"
  },
  "devDependencies": {
    "@babel/core": "7.10.2",
    "@types/circular-dependency-plugin": "5.0.1",
    "@types/enzyme": "3.10.5",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/enzyme-to-json": "1.5.3",
    "@types/express": "4.17.6",
    "@types/express-sslify": "1.2.1",
    "@types/inquirer": "6.5.0",
    "@types/jest": "26.0.0",
    "@types/lodash": "4.14.157",
    "@types/node": "14.0.13",
    "@types/react": "16.9.36",
    "@types/react-dom": "16.9.8",
    "@types/webpack": "4.41.17",
    "@types/webpack-dev-server": "3.11.0",
    "@types/yargs": "15.0.5",
    "@typescript-eslint/eslint-plugin": "3.3.0",
    "@typescript-eslint/parser": "3.3.0",
    "auto-changelog": "2.1.0",
    "babel-loader": "8.1.0",
    "babel-polyfill": "6.26.0",
    "circular-dependency-plugin": "5.2.0",
    "codecov": "3.7.1",
    "dotenv": "8.2.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "enzyme-to-json": "3.5.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-config-typescript": "3.0.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.21.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "2.5.1",
    "husky": "4.2.5",
    "inquirer": "7.2.0",
    "jest": "26.0.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.5",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "simple-git": "2.11.0",
    "ts-jest": "26.1.0",
    "ts-loader": "7.0.5",
    "ts-node": "8.10.2",
    "ttypescript": "1.5.10",
    "typescript": "3.9.5",
    "typescript-transform-paths": "1.1.14",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.11.0",
    "yargs": "15.3.1"
  }
}
