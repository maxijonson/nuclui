{
  "name": "nuclui",
  "version": "0.0.2",
  "description": "A React UI framework, built with TypeScript, to make your developement easier so you can give more focus on your app and less on the design!",
  "main": "dist/index.js",
  "scripts": {
    "start:prod": "node docs/dist-server/server.js",
    "start:dev": "webpack-dev-server --config=\"webpack.config.ts\" --host 0.0.0.0 --env.NODE_ENV=development --port=3000",
    "clean": "rm -rf dist package docs/public/dist docs/dist-server *.tsbuildinfo",
    "compile": "npm run clean && tsc --project tsconfig.nuclui.json",
    "test:publish": "npm run compile && npm run prepublishOnly && npm pack && tar xvzf *.tgz && rm -rf *.tgz && npm run postpublish",
    "test:heroku": "npm run heroku-postbuild && heroku local web",
    "build:docs:dev": "npm run clean && webpack --env.NODE_ENV=development --config=\"webpack.config.ts\"",
    "build:docs:prod": "npm run clean && webpack -p --env.NODE_ENV=production --config=\"webpack.config.ts\"",
    "prepublishOnly": "npm run compile && node scripts/prepublishOnly.js",
    "postpublish": "node scripts/postpublish.js",
    "heroku-postbuild": "npm run build:docs:prod && tsc --project tsconfig.server.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxijonson/nuclui.git"
  },
  "keywords": [
    "react",
    "components",
    "library",
    "framework",
    "UI",
    "front-end",
    "typescript"
  ],
  "author": "Tristan Chin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/maxijonson/nuclui/issues"
  },
  "homepage": "https://github.com/maxijonson/nuclui#readme",
  "files": ["dist"],
  "dependencies": {
    "express": "4.17.1",
    "express-sslify": "1.2.0",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "@babel/core": "7.10.2",
    "@types/circular-dependency-plugin": "5.0.1",
    "@types/express": "4.17.6",
    "@types/express-sslify": "1.2.1",
    "@types/node": "14.0.13",
    "@types/react": "16.9.36",
    "@types/react-dom": "16.9.8",
    "@types/webpack": "4.41.17",
    "@types/webpack-dev-server": "3.11.0",
    "@typescript-eslint/eslint-plugin": "3.3.0",
    "@typescript-eslint/parser": "3.3.0",
    "babel-loader": "8.1.0",
    "babel-polyfill": "6.26.0",
    "circular-dependency-plugin": "5.2.0",
    "dotenv": "8.2.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-config-typescript": "3.0.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.21.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "2.5.1",
    "prettier": "2.0.5",
    "ts-loader": "7.0.5",
    "ts-node": "8.10.2",
    "typescript": "3.9.5",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.11.0"
  }
}
